extends ../layout
block content
    .container
        .row
            table.table.table-hover.table-bordered
                thead
                  tr
                   th 电影名
                   th 导演
                  //- th 上映时间
                tbody
                   each item in movies
                    tr(class="item-id-#{item._id}")
